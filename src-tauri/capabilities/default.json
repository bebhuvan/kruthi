{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "sql:default",
    "store:default",
    "dialog:default",
    "updater:default",
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "$HOME/**"
        }
      ]
    }
  ]
}
